<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
                             "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="personService1" class="com.spring.ex02.PersonServiceImpl">
		<constructor-arg value="이순신"></constructor-arg> <!--인자가 한 개인 생성자로 id가 personService1인 빈을 생성, 생성자로 value인 이순신을 전달하여 name 속성 초기화 -->
	</bean>
	
	<bean id="personService2" class="com.spring.ex02.PersonServiceImpl">
		<constructor-arg value="손흥민"></constructor-arg>
		<constructor-arg value="23"></constructor-arg> <!--인자가 두 개인 생성자로 id가 personService2인 빈을 생성, 생성자로 두 개의 값을 전달하여 name, age 초기화  -->
	</bean>
</beans>

